        "./jalaali-picker.scss": "./dist/jalaali-picker.scss",
        "./options.scss": "./dist/options.scss",
        "./pagination.scss": "./dist/pagination.scss",
        "./pie.scss": "./dist/pie.scss",
        "./simple-pagination.scss": "./dist/simple-pagination.scss",
        "./tab.scss": "./dist/tab.scss",
        "./tile.scss": "./dist/tile.scss",
        "./toggle.scss": "./dist/toggle.scss",
        "./toman.scss": "./dist/toman.scss"
    },
    "files": [
        "dist"
    ],
    "keywords": [
